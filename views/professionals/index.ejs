<!DOCTYPE html>
<html>
  <head>
    <title>Hi professionals</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Hi professionals</h1>
    <a href="/professionals/new">post a new listing</a>
    <br>
<a href="/professionals/lists">See all listings</a>
<br>
<a href="/professionals/myLists">See my listings</a>
    <p> <ul class="right">
      <li>
      <% if (professional) { %>
        <a href="/logout">Log Out</a>
      <% } else { %>
        <a href="/auth/google">Login with Google</a>
      <% } %>
      </li>
    </ul></p>
    <% if (professional) { %>
      <div class="myLists">
        My Listings
          <% listings.forEach( function( list ){ %>
            -------------------------
            <form action="/professionals/index" method="delete"></form>
            <p><%= list.title%></p>
            <p><%= list.content%></p>
          <br>
            <p>Created at <%= list.createdAt%></p>
            <input id="delSub" type="submit" value="del">
            <input id="delSub" type="submit" value="edit">
          </form>
          <br>
          -------------------------
            <br><br><br>

          <% }); %>
      </div>
      <% } %>


    ferjofeoij
  </body>
</html>


