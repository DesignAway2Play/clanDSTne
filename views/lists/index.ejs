<% people.forEach( function( attribute ){ %>
  <% var listings = attribute.list %>
  <% listings.forEach(function (list){ %>
    <p><%= list.title %></p>
    <p><%= list.content %></p>
    <p>Created at <%= list.createdAt %></p>
    <p>Created by <%= list.name %></p>
    <form id="cAnon" action="/lists/<%= list.id %>/comments/new" method="post" >
      <label>Give yourself a handle</label>
      <input type="text" name="anonUser" />
      <label>Say what you want</label>
      <input type="text" name="commContent" required />
  <input id="postComm" type="submit" value="Comment">
  </form>
    <%  }) %>
  <% }); %>

